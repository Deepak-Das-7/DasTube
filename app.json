{
  "expo": {
    "name": "DasTube",
    "slug": "client",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/panther-sm.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/panther.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.deepak7.DasChat"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/panther.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/panther-sm.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "cdc43ece-f702-4db6-8a24-8dd8040e53a3"
      }
    }
  }
}
