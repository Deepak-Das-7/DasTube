{
  "expo": {
    "name": "DasTube",
    "slug": "DasTube",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/panther.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/panther.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.deepak7.DasChat"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/panther.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/panther.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "545ca520-aba8-4e2e-8319-5112c0af2087"
      }
    }
  }
}
